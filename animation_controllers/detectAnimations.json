{
    "format_version": "1.10.0",
    "animation_controllers": {
		"controller.animation.timer": {
			"states": {
				"default": {
					"transitions": [
						{
							"timer": "(1.0)"
						}
					],
					"on_entry": [
						"/function astro/modules/timeplayed",
						"/function astro/utility/main"
					]
				},
				"timer": {
					"transitions": [
						{
							"default": "(1.0)"
						}
					],
					"on_entry": [
						"/function astro/modules/timeplayed",
						"/function astro/utility/main"
					]
				}
			}
		},
        "controller.animation.is_on_ground": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "is_on_ground": "query.is_on_ground"
                        }
                    ],
                    "on_entry": [
                        "/tag @s remove Is_On_Ground",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Is No Longer On Ground\"}]}"
                    ]
                },
                "is_on_ground": {
                    "transitions": [
                        {
                            "default": "!query.is_on_ground"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add Is_On_Ground",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Is On The Ground\"}]}"
                    ]
                }
            }
        },
        "controller.animation.is_levitating": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "is_levitating": "query.is_levitating"
                        }
                    ],
                    "on_entry": [
                        "/tag @s remove LevitationEffect",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 No Longer Has The Levitation Effect\"}]}"
                    ]
                },
                "is_levitating": {
                    "transitions": [
                        {
                            "default": "!query.is_levitating"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add LevitationEffect",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Has The Levitation Effect\"}]}"
                    ]
                }
            }
        },
        "controller.animation.is_gliding": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "is_gliding": "query.is_gliding"
                        }
                    ],
                    "on_entry": [
                        "/tag @s remove Is_Gliding",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Is No Longer Gliding\"}]}"
                    ]
                },
                "is_gliding": {
                    "transitions": [
                        {
                            "default": "!query.is_gliding"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add Is_Gliding",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Is Gliding\"}]}"
                    ]
                }
            }
        },
        "controller.animation.is_jumping": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "is_jumping": "query.is_jumping"
                        }
                    ],
                    "on_entry": [
                        "/tag @s remove Is_Jumping",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Is No Longer Jumping\"}]}"
                    ]
                },
                "is_jumping": {
                    "transitions": [
                        {
                            "default": "!query.is_jumping"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add Is_Jumping",
                        "/tellraw @a[tag=moderating] {\"rawtext\":[{\"text\":\"§4Astro §r §3→ §r\"},{\"selector\":\"@s\"},{\"text\":\"§0 Is Jumping\"}]}"
                    ]
                }
            }
        }
    }
}